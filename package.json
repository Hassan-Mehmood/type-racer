{
  "name": "type-racer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx dotenv -e .env.development.local -- prisma migrate dev",
    "prisma:deploy": "npx dotenv -e .env.development.local -- prisma migrate deploy",
    "db:push": "npx dotenv -e .env.development.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.development.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.development.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.development.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.development.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@vercel/postgres": "^0.5.1",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^7.3.0",
    "lucide-react": "^0.301.0",
    "next": "14.0.4",
    "prisma": "^5.7.1",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
